{
	"name": "@rmenke/postcss-mono-repo-playground--package-a",
	"version": "1.1.1",
	"engines": {
		"node": "^14 || ^16 || >=18"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
		"CHANGELOG.md",
		"LICENSE.md",
